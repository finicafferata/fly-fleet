{
  "name": "fly-fleet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "npm run docs:generate && next build",
    "build:turbo": "npm run docs:generate && next build --turbopack",
    "start": "next start",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up",
    "dev:docker:build": "docker-compose -f docker-compose.dev.yml up --build",
    "lint": "eslint",
    "docs:generate": "node scripts/generate-api-docs.js",
    "docs:watch": "node scripts/generate-api-docs.js --watch",
    "docs:serve": "swagger-ui-serve openapi.yaml",
    "docs:serve-dev": "swagger-ui-serve -p 8080 openapi.yaml",
    "docs:build-static": "redoc-cli build openapi.yaml --output docs/api.html",
    "docs:validate": "swagger-parser validate openapi.yaml",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/",
    "test:integration": "jest --testPathPattern=tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "playwright test tests/e2e/accessibility.spec.ts",
    "test:performance": "playwright test tests/performance/",
    "test:lighthouse": "lhci autorun",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e && npm run test:lighthouse",
    "screenshot:compare": "node scripts/screenshot-comparison.js",
    "visual:compare": "node scripts/visual-comparison.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.2",
    "@react-email/render": "^1.2.3",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "next": "15.5.3",
    "next-intl": "^4.3.9",
    "postcss": "^8.5.6",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "resend": "^6.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.3",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.41.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axe-core": "^4.8.4",
    "chokidar": "^4.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "redoc-cli": "^0.13.21",
    "swagger-parser": "^10.0.3",
    "typescript": "^5"
  }
}
