{
  "name": "fly-fleet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run docs:generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "docs:generate": "node scripts/generate-api-docs.js",
    "docs:watch": "node scripts/generate-api-docs.js --watch",
    "docs:serve": "swagger-ui-serve openapi.yaml",
    "docs:serve-dev": "swagger-ui-serve -p 8080 openapi.yaml",
    "docs:build-static": "redoc-cli build openapi.yaml --output docs/api.html",
    "docs:validate": "swagger-parser validate openapi.yaml"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@prisma/client": "^6.16.2",
    "@react-email/render": "^1.2.3",
    "@tailwindcss/forms": "^0.5.10",
    "clsx": "^2.1.1",
    "next": "15.5.3",
    "next-intl": "^4.3.9",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "resend": "^6.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chokidar": "^4.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "js-yaml": "^4.1.0",
    "redoc-cli": "^0.13.21",
    "swagger-parser": "^10.0.3",
    "swagger-ui-serve": "^3.52.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
